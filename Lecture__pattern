// create a pattern in the same row and column

#include <iostream>
using namespace std;
int main()
{
    int n ;
    cout<<" Enter the value number "<<endl;
    cin>>n;
    int i=1;
    while (i<=n)
    {
        int j=1;
        while (j<=n)
        {
           cout<<"*";
           j=j+1;
        }
        cout<<endl;
         i=i+1;

    }

//Output
Enter the value number 
5
*****
*****
*****
*****
*****
